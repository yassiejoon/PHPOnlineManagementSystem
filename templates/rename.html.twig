{# empty Twig template #}
{% extends "master.html.twig" %}

{% block title %} File rename {% endblock %}

{% block content %}
    
    {% if errorList %}
    <ul class="errorList">
        {% for error in errorList %}
            <li> {{ error }} </li>
            {% endfor %}
    </ul>
    {% endif %}
    
    <form method="post">
        File Name: <input type="type" name="filename" value="{{f.filename}}"><br>
        <input type="submit" value="Rename File">
    </form>    
 
{% endblock %}