{% extends "master.html.twig" %}

{% block title %} File download {% endblock %}

{% block content %}
    
    {% if errorList %}
    <ul class="errorList">
        {% for error in errorList %}
            <li> {{ error }} </li>
            {% endfor %}
    </ul>
    {% endif %}
    
    <form method="post">
        File Name: <input type="text" name="filename" value="{{f.filename}}"><br>
        <input type="submit" value="Download File">
    </form>    
    
    <a href="/list">return</a>
{% endblock %}