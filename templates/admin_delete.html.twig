{# empty Twig template #}
{% extends "master.html.twig" %}

{% block title %} User delete {% endblock %}

{% block content %}
    
    {% if errorList %}
    <ul class="errorList">
        {% for error in errorList %}
            <li> {{ error }} </li>
            {% endfor %}
    </ul>
    {% endif %}
    
    <form method="post">
        User Name: <input type="text" name="name" value="{{u.name}}"><br>
        <input type="submit" value="Delete User">
    </form>    
 
{% endblock %}
